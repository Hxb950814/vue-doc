{
    "compilerOptions": {
        "target": "es2018",
        "module": "esnext",
        "moduleResolution": "node",
        "downlevelIteration": true,
        "lib": [
            "esnext",
            "esnext.asynciterable",
            "dom"
        ],
        "esModuleInterop": true,
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "./*"
            ],
            "@/*": [
                "./*"
            ],
            "@common/*": [
                "./../fushui-common/*"
            ],
            "@root/*": [
                "./../*"
            ]
        },
        "types": [
        ]
    },
    "exclude": [
        "node_modules",
        ".nuxt",
        "dist"
    ]
}
